{
    "name": "pop-schema/media",
    "type": "library",
    "description": "Adds support for media",
    "keywords": [
        "PoP",
        "media"
    ],
    "homepage": "https://github.com/PoPSchema/media",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.4",
        "getpop/engine": "dev-master",
        "pop-schema/schema-commons": "dev-master",
        "pop-schema/migrate-media": "dev-master"
    },
    "require-dev": {
        "pop-schema/users": "dev-master",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": ">=9.3",
        "rector/rector": "dev-master#59547a5cbe7845c2ca493ede481e1141e53df079",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "suggest": {
        "pop-schema/users": "Integration with Users"
    },
    "autoload": {
        "psr-4": {
            "PoPSchema\\Media\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PoPSchema\\Media\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse -c phpstan.neon.dist",
        "downgrade-code": "rector process --dry-run"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
